{
  "name": "sierra-handlebars",
  "version": "0.1.0",
  "description": "Handlebars integration for Sierra",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjohnsonaz/sierra-handlebars.git"
  },
  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/SierraHandlebars.js",
  "typings": "dist/SierraHandlebars.d.ts",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node --inspect ./dist/examples/SierraHandlebarsTest.js\"",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "rimraf ./dist && tsc --declaration",
    "test": "tsc && mocha ./dist/**/*.test.js",
    "watch": "tsc --watch --declaration",
    "docs": "docsify serve docs"
  },
  "dependencies": {
    "handlebars": "4.7.6"
  },
  "peerDependencies": {
    "sierra": "^0.5.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chai-http": "4.2.0",
    "@types/handlebars": "4.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.2",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "docsify-cli": "^4.4.0",
    "mocha": "7.1.1",
    "rimraf": "3.0.2",
    "sierra": "^0.5.0",
    "tsc-watch": "4.2.3",
    "typescript": "3.8.3"
  },
  "keywords": [
    "sierra",
    "handlebars",
    "mvc",
    "view"
  ]
}
