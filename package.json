{
  "name": "sierra-handlebars",
  "version": "0.1.0",
  "description": "Handlebars integration for Sierra",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjohnsonaz/sierra-handlebars.git"
  },
  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/SierraHandlebars.js",
  "typings": "dist/SierraHandlebars.d.ts",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node --inspect ./dist/examples/SierraHandlebarsTest.js\"",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "prepublishOnly": "rimraf ./dist && tsc --declaration",
    "test": "jest --coverage --verbose",
    "watch": "tsc --watch --declaration",
    "docs": "docsify serve docs"
  },
  "dependencies": {
    "handlebars": "4.7.7"
  },
  "peerDependencies": {
    "sierra": "^0.5.7"
  },
  "devDependencies": {
    "@types/handlebars": "4.1.0",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@types/supertest": "^2.0.10",
    "docsify-cli": "4.4.1",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "sierra": "^0.5.7",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.3"
  },
  "keywords": [
    "sierra",
    "handlebars",
    "mvc",
    "view"
  ],
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
